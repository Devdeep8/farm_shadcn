{
  "name": "deva",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "prisma": "yarn workspace db prisma",
    "prisma:migrate": "yarn prisma migrate dev --schema packages/db/prisma/schema.prisma",
    "prisma:generate": "yarn prisma generate --schema packages/db/prisma/schema.prisma",
    "prisma:studio": "yarn prisma studio  packages/db/prisma/schema.prisma"
  },
  "devDependencies": {
    "prettier": "^3.6.0",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
